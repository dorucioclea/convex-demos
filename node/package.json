    "dev:client": "vite --open",
    "dev:init": "convex dev --once"
  },
  "dependencies": {
    "convex": "1.6.3",
    "dotenv": "^16.0.3",
    "ws": "^8.11.0",
    "ts-node": "~10.9.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.12",
    "typescript": "~5.0.3",
    "npm-run-all": "^4.1.5"
  }
}
